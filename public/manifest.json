{
  "author": "wsbaser",
  "manifest_version": 2,
  "name": "Trend State",
  "version": "0.0.3",
  "description": "Helps to define quality of the trend for forex pairs.",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
   },
  "background": {
      "scripts": [ "assets/background.js" ]
  },
  "permissions": ["background", "storage", "tabs", "notifications", "activeTab", "http://92.53.120.34:5000/*"],
  "web_accessible_resources": [
    "assets/images/icon16.png"
  ],
  "externally_connectable": {
    "matches": [ ]
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "assets/popup/index.html"
  }
}